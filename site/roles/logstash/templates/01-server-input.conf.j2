input{

    file {
      type => "syslog"
      path => [ "/var/log/messages", "/var/log/syslog", "/var/log/*.log" ]
      sincedb_path => "/var/cache/logstash/syslog.sincedb"
    }
  
    rabbitmq {
    user => 'logstash'
    password => 'logstash'
    durable => true
    exchange => 'exchangelogstash'
    host => ["{{ rabbitmq_host }}"]
    key => 'logs-%{host}'
    port => 5672
    queue => 'queuelogstash'
    vhost => '/logstash'
    }












}